{
  "name": "remix-template-cloudflare-pages",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "test": "vitest run",
    "dev:remix": "remix watch",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --binding $(cat .env)",
    "dev": "concurrently \"npm run dev:css\" \"remix build && run-p dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "test:playwright": "playwright test"
  },
  "dependencies": {
    "@remix-run/cloudflare": "1.5.1",
    "@remix-run/cloudflare-pages": "1.5.1",
    "@remix-run/react": "1.5.1",
    "@supabase/supabase-js": "^1.35.3",
    "@types/uuid": "^8.3.4",
    "cross-env": "7.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remix-auth": "3.2.2",
    "remix-auth-google": "1.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.11.0",
    "@playwright/test": "1.22.2",
    "@remix-run/dev": "1.5.1",
    "@remix-run/eslint-config": "1.5.1",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "10.4.7",
    "concurrently": "7.2.1",
    "eslint": "8.17.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "tailwindcss": "3.0.24",
    "typescript": "4.7.3",
    "vitest": "0.14.1",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
