{
  "name": "remix-template-cloudflare-pages",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "test": "vitest",
    "dev:remix": "remix watch",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --binding $(cat .env)",
    "dev": "concurrently \"npm run dev:css\" \"remix build && run-p dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.4.3",
    "@remix-run/cloudflare-pages": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.2.2",
    "remix-auth-google": "^1.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.10.0",
    "@playwright/test": "^1.22.2",
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "vitest": "^0.12.9",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
