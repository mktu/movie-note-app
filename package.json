{
  "name": "remix-template-cloudflare-pages",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint app/features/** --ext .ts,.tsx",
    "test": "vitest run",
    "dev:remix": "remix watch",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --binding $(cat .env) --kv ImdbInfo --kv TmdbInfo",
    "dev": "concurrently \"npm run dev:css\" \"remix build && run-p dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "test:playwright": "playwright test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@headlessui/react": "1.7.2",
    "@lexical/react": "0.4.1",
    "@remix-run/cloudflare": "1.7.1",
    "@remix-run/cloudflare-pages": "1.7.1",
    "@remix-run/react": "1.7.1",
    "@supabase/supabase-js": "1.35.6",
    "@types/uuid": "8.3.4",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "date-fns": "2.29.3",
    "i18next": "21.9.2",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-http-backend": "1.4.1",
    "i18next-resources-to-backend": "1.0.0",
    "lexical": "0.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-i18next": "11.18.5",
    "react-intersection-observer": "9.4.0",
    "react-toastify": "9.0.8",
    "remix-auth": "3.2.2",
    "remix-auth-google": "1.1.0",
    "remix-i18next": "4.1.1",
    "uuid": "9.0.0",
    "zod-form-data": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@cloudflare/workers-types": "3.16.0",
    "@playwright/test": "1.25.2",
    "@remix-run/dev": "1.7.1",
    "@remix-run/eslint-config": "1.7.1",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/addon-postcss": "3.0.0-alpha.1",
    "@storybook/builder-webpack4": "6.5.10",
    "@storybook/manager-webpack4": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@types/i18next-browser-languagedetector": "3.0.0",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "10.4.11",
    "babel-loader": "8.2.5",
    "concurrently": "7.4.0",
    "eslint": "8.23.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-tailwindcss": "3.6.2",
    "msw": "0.47.3",
