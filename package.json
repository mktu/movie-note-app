{
  "name": "remix-template-cloudflare-pages",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint app/features/** --ext .ts,.tsx",
    "test": "vitest run",
    "dev:remix": "remix dev --manual -c \"npm run start\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev --compatibility-date=2023-08-14 ./public --binding $(cat .env) --kv ImdbInfo --kv TmdbInfo --kv MovieNoteIds --kv OgpInfo --r2=MovieNoteApp --r2=MovieNoteImage",
    "dev": "concurrently \"npm run dev:css\" \"remix build && run-p dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "test:playwright": "playwright test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
