{
  "name": "remix-template-cloudflare-pages",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "lint": "eslint app/features/** --ext .ts,.tsx",
    "test": "vitest run",
    "dev:remix": "remix watch",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --binding $(cat .env)",
    "dev": "concurrently \"npm run dev:css\" \"remix build && run-p dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "test:playwright": "playwright test"
  },
  "dependencies": {
    "@headlessui/react": "1.6.6",
    "@remix-run/cloudflare": "1.6.4",
    "@remix-run/cloudflare-pages": "1.6.4",
    "@remix-run/react": "1.6.4",
    "@supabase/supabase-js": "1.35.4",
    "@types/uuid": "8.3.4",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "i18next": "21.8.13",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-http-backend": "1.4.1",
    "i18next-resources-to-backend": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.33.1",
    "react-i18next": "11.18.0",
    "react-split": "2.0.14",
    "remix-auth": "3.2.2",
    "remix-auth-google": "1.1.0",
    "remix-i18next": "4.1.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.14.0",
    "@playwright/test": "1.23.2",
    "@remix-run/dev": "1.6.4",
    "@remix-run/eslint-config": "1.6.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "10.4.7",
    "concurrently": "7.2.2",
    "eslint": "8.19.0",
    "eslint-plugin-tailwindcss": "3.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "tailwindcss": "3.1.5",
    "typescript": "4.7.4",
    "vitest": "0.16.0",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14"
  }
}
